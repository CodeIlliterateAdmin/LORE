{{! Details Tab }}
<section
  class='tab details {{tab.cssClass}}'
  data-group='primary'
  data-tab='details'
>
  {{! As you add new fields, add them in here! }}
  <div class='resource'>
    <label class='resource-label'>{{localize 'LORE.Item.Magick.FIELDS.level.label'}}</label>
    <input
      type='text'
      name='system.magickLevel'
      value='{{system.magickLevel}}'
      data-dtype='Number'
    />
  </div>
</section>